    "chart.js": "^1.1.1",
    "express": "^4.13.4",
    "google-maps-react": "^2.0.2",
    "point-in-polygon": "^1.0.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-google-map-draw-filter": "^1.2.2",
    "multi-slider": "^4.1.0",
    "react": "^15.6.2",
    "react-chartjs": "^1.2.0",
    "react-dom": "^15.6.2",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "recharts": "^1.0.0-beta.10",
    "redux": "^3.5.2",
    "redux-thunk": "^2.2.0"
  }
}
